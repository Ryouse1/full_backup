prefix=/usr
exec_prefix=/usr
libdir=/usr/lib/x86_64-linux-gnu
includedir=/usr/include
ccompiler=/usr/bin/cc

Name: netCDF
Description: NetCDF Client Library for C
URL: http://www.unidata.ucar.edu/netcdf
Version: 4.8.1
Libs: -L${libdir} -lnetcdf
Libs.private: -lhdf5_hl -lhdf5 -lcrypto -lcurl -lpthread -lsz -lz -ldl -lm -L/usr/lib/x86_64-linux-gnu/hdf5/serial
Cflags: -I${includedir} -I/usr/include/hdf5/serial
